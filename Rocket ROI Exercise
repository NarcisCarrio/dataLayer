<!--Scroll-->

window.onscroll = function() {scrollEvent25();scrollEvent50();scrollEvent75();};

function scrollEvent25() {
  if (document.body.scrollTop > (0.25*document.body.scrollHeight)) {
    dataLayer.push({'event': 'scroll-25'});
  }
}

function scrollEvent50() {
  if (document.body.scrollTop > (0.5*document.body.scrollHeight)) {
    dataLayer.push({'event': 'scroll-50'});
  }
}


function scrollEvent75() {
  if (document.body.scrollTop > (0.75*document.body.scrollHeight)) {
    dataLayer.push({'event': 'scroll-75'});
  }
}

function scrollEvent100() {
  if (document.body.scrollTop = document.body.scrollHeight) {
    dataLayer.push({'event': 'scroll-75'});
  }
}


